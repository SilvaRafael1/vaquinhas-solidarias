// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "./prisma/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Vaquinha {
  id    Int     @id @default(autoincrement())
  name  String
  desc  String
  url   String
  image String?
}
